{
  "name": "makeandship/plugin-media-text-extractor",
  "description": "Extract text from media library attachments and store them for further processing",
  "require": {
    "makeandship/logging-php": "v1.0.4"
  },
  "require-dev": {},
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "makeandship/logging-php",
        "version": "v1.0.4",
        "type": "library",
        "source": {
          "url": "https://github.com/SpecialistPharmacyService/logging-php.git",
          "type": "git",
          "reference": "v1.0.4"
        },
        "require": {
          "composer/installers": "^2.0"
        }
      }
    }
  ],
  "authors": [
    {
      "name": "mthomsit",
      "email": "mark@makeandship.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
